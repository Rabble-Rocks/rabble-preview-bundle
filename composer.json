{
    "name": "rabble/preview-bundle",
    "type": "symfony-bundle",
    "description": "Adds a preview to the content section when editing or creating a page in the content bundle.",
    "keywords": ["rabble", "admin", "preview", "cms"],
    "homepage": "https://rabble.rocks",
    "license": "MIT",
    "authors": [
        {
            "name": "Rachel Snijders",
            "email": "rainbowvision.bn@gmail.com"
        }
    ],
    "require": {
        "php": "^7.4 || ^8.0",
        "rabble/content-bundle": "*"
    },
    "autoload": {
        "psr-4": { "Rabble\\PreviewBundle\\": "src/PreviewBundle" }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
